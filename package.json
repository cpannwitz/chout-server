{
  "name": "@chout/server",
  "version": "0.0.1",
  "description": "NodeJS server powered by NestJS for Chout",
  "author": {
    "email": "ch.von.pa@gmail.com",
    "name": "Christian Pannwitz",
    "url": "https://pnwtz.me"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:docker": "docker-compose up -V",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.5",
    "@nestjs/config": "^0.1.0",
    "@nestjs/core": "^6.11.5",
    "@nestjs/platform-express": "^6.11.5",
    "@nestjs/swagger": "^4.2.3",
    "@nestjs/typeorm": "^6.2.0",
    "@timberio/node": "^0.35.0",
    "@timberio/winston": "^0.35.0",
    "nest-winston": "^1.3.1",
    "node-mocks-http": "^1.8.1",
    "pg": "^7.18.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.1",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "^0.2.22",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.1",
    "@nestjs/schematics": "^6.9.1",
    "@nestjs/testing": "^6.11.5",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
