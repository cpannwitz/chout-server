{
  "name": "chout-server",
  "version": "0.0.1",
  "description": "nodejs server for chout",
  "private": true,
  "author": {
    "email": "choutapp@gmail.com",
    "name": "Chout",
    "url": "https://chout.app"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.8.0",
    "yarn": "1.x"
  },
  "main": "./dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production node --optimize_for_size --max_old_space_size=460 --gc_interval=100 ./dist/index.js",
    "start:dev": "NODE_ENV=development nodemon --exec ts-node --files src/index.ts",
    "build": "NODE_ENV=production npm-run-all clean tsc copy:static migration:run",
    "build:dev": "NODE_ENV=production npm-run-all clean tsc copy:static migration:run:dev",
    "build:test": "NODE_ENV=production npm-run-all clean tsc copy:static migration:run:test",
    "copy:static": "cp -R src/static dist",
    "tsc": "tsc",
    "clean": "rimraf ./dist",
    "test": "NODE_ENV=test CI=true jest",
    "test:dev": "NODE_ENV=test jest --watch",
    "migration:run": "typeorm migration:run -c production",
    "migration:run:test": "typeorm migration:run -c test",
    "migration:run:dev": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:gen": "typeorm migration:generate -c production -n migration",
    "migration:gen:test": "typeorm migration:generate -c test -n migration",
    "migration:gen:dev": "ts-node ./node_modules/typeorm/cli.js migration:generate -n migration",
    "migration:create": "typeorm migration:create -c production -n migration",
    "migration:revert": "typeorm migration:revert",
    "migration:revert:dev": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.1.2",
    "@sentry/node": "^5.6.0-beta.0",
    "argon2": "^0.24.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.9.1",
    "compression": "^1.7.4",
    "connect-redis": "^3.4.2",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "cron": "^1.7.1",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "express-jwt": "^5.3.1",
    "express-rate-limit": "^5.0.0",
    "express-status-monitor": "^1.2.6",
    "express-winston": "^3.2.1",
    "got": "^9.6.0",
    "helmet": "^3.20.0",
    "http-errors": "^1.7.3",
    "ioredis": "^4.11.2",
    "ioredis-mock": "^4.12.1",
    "jsonwebtoken": "^8.5.1",
    "logdna-winston": "^2.2.1",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-custom": "^1.0.5",
    "passport-facebook-token": "^3.3.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter-token": "^1.3.0",
    "pg": "^7.12.0",
    "pug": "^2.0.4",
    "rate-limit-redis": "^1.6.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "response-time": "^2.3.2",
    "serve-favicon": "^2.5.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.3.0-alpha.23",
    "typeorm-typedi-extensions": "^0.2.3",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/connect-timeout": "^0.0.34",
    "@types/cors": "^2.8.5",
    "@types/debug": "^4.1.4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-fileupload": "^1.1.0",
    "@types/express-rate-limit": "^3.3.1",
    "@types/express-winston": "^3.0.3",
    "@types/helmet": "^0.0.43",
    "@types/http-errors": "^1.6.1",
    "@types/ioredis": "^4.0.13",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/morgan": "^1.7.36",
    "@types/multer": "^1.3.7",
    "@types/node": "^12.6.8",
    "@types/passport": "^1.0.0",
    "@types/passport-facebook-token": "^0.4.33",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.13",
    "@types/request": "^2.48.2",
    "@types/response-time": "^2.3.3",
    "@types/serve-favicon": "^2.2.30",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.8.0",
    "jest-chain": "^1.1.2",
    "jest-extended": "^0.11.2",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  }
}
