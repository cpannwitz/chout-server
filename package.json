{
  "name": "@chout/server",
  "version": "0.0.1",
  "description": "NodeJS server powered by NestJS for Chout",
  "author": {
    "email": "ch.von.pa@gmail.com",
    "name": "Christian Pannwitz",
    "url": "https://pnwtz.me"
  },
  "license": "MIT",
  "engines": {
    "node": ">=13.0.0",
    "yarn": "1.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:docker": "docker-compose up -V -d && docker-compose logs -f node",
    "start:docker:full": "docker-compose up --build -V -d && docker-compose logs -f node",
    "stop:docker": "docker-compose down",
    "start:debug": "NODE_OPTIONS='--trace-warnings' nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "start:prod:migrations": "run-s migrations:run:prod start:prod",
    "lint": "./node_modules/.bin/eslint \"src/**/*\" --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css}\"",
    "version": "auto-changelog -p -u -t keepachangelog && git add .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:reset": "run-s db:clean migrations:run db:seed",
    "db:clean": "rimraf .data",
    "db:seed": "ts-node src/_db/run-seed",
    "schema:generate:dev": "ts-node src/generate-typings",
    "schema:generate:prod": "node dist/generate-typings",
    "migrations:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n $NAME",
    "migrations:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n $NAME",
    "migrations:show": "ts-node ./node_modules/typeorm/cli.js migration:show",
    "migrations:run:dev": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrations:run:prod": "typeorm migration:run"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.4.1",
    "@mikro-orm/core": "^4.0.4",
    "@mikro-orm/nestjs": "^4.0.0",
    "@mikro-orm/postgresql": "^4.0.4",
    "@nestjs/common": "^7.4.4",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.4",
    "@nestjs/graphql": "^7.6.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.4",
    "@nestjsplus/redirect": "^1.0.0",
    "apollo-server-express": "^2.17.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.3.6",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^6.2.3",
    "helmet": "^4.1.1",
    "nest-access-control": "^2.0.2",
    "nest-raven": "^7.0.0",
    "nestjs-pino": "^1.2.0",
    "nestjs-redis": "^1.2.8",
    "node-mocks-http": "^1.9.0",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.3",
    "pino-pretty": "^4.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.4",
    "type-graphql": "^1.0.0",
    "typeorm": "^0.2.26"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.4.4",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/graphql": "^14.5.0",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.11.1",
    "@types/passport": "^1.0.4",
    "@types/passport-google-oauth20": "^2.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "keepachangelog": "^0.7.2",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.0",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
